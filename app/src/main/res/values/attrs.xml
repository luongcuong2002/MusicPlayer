<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RectangleRelativeLayout">
        <attr name="rectAspectRatio" format="float"/>
        <attr name="dependOnDimension" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSwitch">
        <attr name="checked" format="boolean" />
        <attr name="trackActive" format="reference" />
        <attr name="trackInactive" format="reference" />
        <attr name="thumbActive" format="reference" />
        <attr name="thumbInactive" format="reference" />
        <attr name="invokeDefaultOnClick" format="boolean" />
    </declare-styleable>
</resources>